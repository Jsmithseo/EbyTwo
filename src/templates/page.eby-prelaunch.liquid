{% layout 'prelaunch' %}

<meta name="viewport" content="width=device-width, initial-scale=1">

<div class="prelaunch-landing">

  <div class="email-box blue">
    <p>Limited quantities available.<br/>Get in line now!</p>
    <button class="reserve-spot">Reserve My Spot</button>
  </div>

  <div class="form-activated">
    <div class="close"></div>
    <div class="logo">
      <a href="/"><img src="{{ 'logo-blue.svg' | asset_url }}" alt="EBY" title="EBY"/></a>
    </div>

    <h2>Seamless Cotton</h2>
    <div class="form">
      <h3> Limited quantities available.<br/>Reserve your spot in line now!</h3>

      <form id="reservation-form">

        <input type="hidden" name="g" value="U63fp3">
        <input type="hidden" name="$fields" value="$consent">
        <input type="hidden" name="$list_fields" value="$consent">
        <div id="email-block">
          <label id="reservationEmailLabel" for="k_id_email">Email <span>(required)</span></label>
          <input type="email" name="email" id="k_id_email" required>
        </div>

        <label for="name">First Name <span>(optional)</span></label>
        <input id="name" type="text" name="$first_name">

        <label for="phone">Phone <span>(optional)</span></label>
        <input id="phone" type="text" name="$phone_number">
        <button type="submit">Submit</button>
        <div class="checkbox">
          <input type="checkbox" name="$consent" id="consent-email" required checked/>
          <label for="consent-email">By submiting my info, I agree to recieve marketing com’s per EBY’s <a href="https://shop.join-eby.com/pages/terms-of-use" target="_blank">Terms of Service</a> and <a href="https://shop.join-eby.com/pages/privacy-policy" target="_blank">Privacy Policy</a>.</label>
        </div>
      </form>
    </div>

    <div class="success" id="reservation-success">
      <h2>You’re Reserved!</h2>
      <div class="line"></div>
      <p>We’ll see you on March 5th!</p>
      <a href="https://shop.join-eby.com/collections" target="_blank">Shop EBY</a>
    </div>
  </div>
  <div class="prelaunch-landing-container">

    <div class="row">
      <div class="col-sm-6 sm-order-1">
        <div class="right-column">
          <div class="intro">
            <div class="logo">
